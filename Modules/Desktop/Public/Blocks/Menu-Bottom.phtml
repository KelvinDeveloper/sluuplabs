});

$('.infoUsersBlock .nav li').click(function(){
  $('.infoUsersBlock .nav li.active').removeClass('active');
  $(this).addClass('active');

  $('nav .tab').hide();
  $('nav .tab[for="' + $(this).attr('id') + '"]').show();
});

</script>